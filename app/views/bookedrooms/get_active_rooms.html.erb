<tbody>
<table>
  <thead>
  <tr>
    <th>Roomno</th>
    <th>Building</th>
    <th>Size</th>
    <th>Status</th>
    <th>Booking Time</th>
    <th>Action</th>
  </tr>
  </thead>

  <tbody>c
  <% @rooms.each do |bookedroom| %>
      <tr>
        <%@temp_room=Room.where("roomno = ?",bookedroom.room_id).take%>

        <td><%= bookedroom.room_id %></td>
        <td><%= @temp_room.building %></td>
        <td><%= @temp_room.size %></td>
        <td>Booked</td>
        <td><%= bookedroom.booking_time %></td>
        <td><%= link_to 'Cancel Booking', cancel_booking_path(:id => bookedroom.room_id) %></td>
      </tr>
  <% end %>
  </tbody>
</table>
