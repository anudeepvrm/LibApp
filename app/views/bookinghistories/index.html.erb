<p id="notice"><%= notice %></p>

<h1>Bookinghistories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookinghistories.each do |bookinghistory| %>
      <tr>
        <td><%= link_to 'Show', bookinghistory %></td>
        <td><%= link_to 'Edit', edit_bookinghistory_path(bookinghistory) %></td>
        <td><%= link_to 'Destroy', bookinghistory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookinghistory', new_bookinghistory_path %>
